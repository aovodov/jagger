<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:custom="urn:import:com.griddynamics.jagger.webclient.client.components">
    <ui:with type="com.griddynamics.jagger.webclient.client.resources.JaggerResources" field="res"/>
    <g:DockLayoutPanel addStyleNames="{res.css.plotPanel}">
        <g:north size="20">
            <custom:SummaryButtonsPanel ui:field="summaryButtonsPanel" height="100%"/>
        </g:north>
        <g:center height="100%">
            <g:ScrollPanel ui:field="scrollPanel">
                <g:VerticalPanel ui:field="pane" height="100%" width="100%"/>
            </g:ScrollPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>